<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/anketa_frame"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    <!-- Telephones Layout -->
	<LinearLayout 
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
     <RelativeLayout
        android:layout_width="0dip"
        android:layout_height="wrap_content"
        android:paddingLeft="5px"
        android:paddingBottom="4px"
        android:layout_weight="1">
    <TextView
            android:id="@+id/flat_param_name_tel_left"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/anketa_view_field_name_height"
            android:textSize="@dimen/anketa_view_field_name_text_size"
            android:textColor="@android:color/black"/>
    <TextView
            android:id="@+id/flat_param_tel_left"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/anketa_view_field_height"
            android:minLines="1"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/anketa_view_field_margin_top"
            android:layout_marginLeft="5px"
            android:layout_marginRight="5px"
            android:textSize="@dimen/anketa_view_field_text_size"
            android:paddingLeft="5px"
            android:background="#FFCCCCCC"
            android:textColor="@android:color/black"/>
	</RelativeLayout>
     <RelativeLayout
        android:layout_width="0dip"
        android:layout_height="wrap_content"
        android:paddingLeft="3px"
        android:paddingRight="5px"
        android:paddingBottom="4px"
        android:layout_weight="1">
    <TextView
            android:id="@+id/flat_param_name_tel_right"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/anketa_view_field_name_height"
            android:textSize="@dimen/anketa_view_field_name_text_size"
            android:textColor="@android:color/black"/>
    <TextView
            android:id="@+id/flat_param_tel_right"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:minHeight="@dimen/anketa_view_field_height"
            android:minLines="1"
            android:gravity="center_vertical"
            android:layout_marginTop="@dimen/anketa_view_field_margin_top"
            android:layout_marginLeft="5px"
            android:layout_marginRight="5px"
            android:textSize="@dimen/anketa_view_field_text_size"
            android:paddingLeft="5px"
            android:background="#FFCCCCCC"
            android:textColor="@android:color/black"/>
	</RelativeLayout>
    </LinearLayout>
    <!-- Names Layout -->
